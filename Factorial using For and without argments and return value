#include<stdio.h>
void disp();
int main()
{
    disp();
return 0;
}

void disp()
    {
        int a,fact,i;
        printf("Enter Value Of a = ");
        scanf("%d",&a);
        i=1;
        fact=1;
        for (i=1;i<=a;i++)
        {
            fact=fact*i;
        }
        printf("The Factorial Of Given Number Is = %d",fact);
    }
