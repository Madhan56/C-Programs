#include <stdio.h>
int main()
{
    int s1, s2, s3, s4, s5,average;
    printf("Enter The Value Of s1:");
    scanf("%d,%d,%d,%d,%d",&s1);
    printf("\nEnter The Value Of s2:");
    scanf("%d",&s2);
    printf("\nEnter The Value Of s3:");
    scanf("%d",&s3);
    printf("\nEnter The Value Of s4:");
    scanf("%d",&s4);
    printf("\nEnter The Value Of s5:");
    scanf("%d",&s5);
    printf("\nTotal Marks,%d",s1+s2+s3+s4+s5);
    average=(s1+s2+s3+s4+s5)/5;
    printf("\nAverage,%d",average);
    if (average>=60)
    printf("\nThe Student Has Passed");
    else 
    printf("\nThe Student Has Failed");
    return 0;
}
